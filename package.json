{
  "name": "latex-plus",
  "main": "./lib/main",
  "version": "0.6.2",
  "description": "Productivity tools for pdfLaTeX, LuaLaTeX, and XeLaTeX flavors of LaTeX.",
  "keywords": [
    "tex",
    "latex",
    "pdflatex",
    "xelatex",
    "lualatex"
  ],
  "repository": "https://github.com/andrewrynhard/atom-latex-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "atom-message-panel": "*"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "activationCommands": {
    "atom-workspace": "latex-plus:compile"
  }
}
